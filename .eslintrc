{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "extends": ["prettier", "airbnb"],
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react", "html", "import", "prettier", "jsx-a11y"],
  "rules": {
    "react/prop-types": 0,
    "react/jsx-uses-vars": [2],
    "no-console": 0,
    "no-unused-vars": "warn",
    "comma-dangle": 0,
    "react/jsx-filename-extension": 0,
    "semi": 0
  }
}
